#reader(lib "htdp-intermediate-lambda-reader.ss" "lang")((modname uncurry) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f ())))
;; contract: uncurry: (X -> (Y -> Z)) -> (X Y -> Z)
(define (uncurry X)
  ((lambda (X Y) (Y)) X (X))

(X):= (lambda (Y) Z)
(Y):= Z